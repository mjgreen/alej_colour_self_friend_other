      <CodeComponent name="assign_keymap_at_random_and_mk_instructions" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# choose one of 6 combinations of j,k,l mapping to redself; bluefriend; othergreen&amp;#10;#redself   bluefriend  greenother&amp;#10;#1=j              k                    l&amp;#10;#2=j              l                     k&amp;#10;#3=k             j                     l&amp;#10;#4=k             l                     j&amp;#10;#5=l              j                     k&amp;#10;#6=l              k                    j&amp;#10;&amp;#10;import random&amp;#10;if expInfo['group']:&amp;#10;    mapping_number = int(expInfo['group'])&amp;#10;else:&amp;#10;    mapping_number = random.randint(1,6)&amp;#10;instruct_color_condition = &quot;oops&quot;&amp;#10;instruct_identity_condition = &quot;oops&quot;&amp;#10;match mapping_number:&amp;#10;    case 1:&amp;#10;        redself    = &quot;j&quot;&amp;#10;        bluefriend = &quot;k&quot;&amp;#10;        greenother = &quot;l&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case 2:&amp;#10;        redself    = &quot;j&quot;&amp;#10;        bluefriend = &quot;l&quot;&amp;#10;        greenother = &quot;k&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case 3:&amp;#10;        redself    = &quot;k&quot;&amp;#10;        bluefriend = &quot;j&quot;&amp;#10;        greenother = &quot;l&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case 4:&amp;#10;        redself    = &quot;k&quot;&amp;#10;        bluefriend = &quot;l&quot;&amp;#10;        greenother = &quot;j&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case 5:&amp;#10;        redself    = &quot;l&quot;&amp;#10;        bluefriend = &quot;j&quot;&amp;#10;        greenother = &quot;k&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case 6:&amp;#10;        redself    = &quot;l&quot;&amp;#10;        bluefriend = &quot;k&quot;&amp;#10;        greenother = &quot;j&quot;&amp;#10;        instruct_color_condition = \&amp;#10;        &quot;In the following task, you will need to report the color of the word.&quot; + \&amp;#10;        &quot;\nIf the word is presented in green, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is presented in blue, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is presented in red, press &quot; + redself + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;        instruct_identity_condition = \&amp;#10;        &quot;In the following task, you will need to report the identity of the word.&quot; + \&amp;#10;        &quot;\nIf the word is other, press &quot; + greenother + \&amp;#10;        &quot;\nIf the word is friend, press &quot; + bluefriend + \&amp;#10;        &quot;\nIf the word is self, press &quot; + redself + \&amp;#10;        &quot;\n\npress the space key to start&quot;&amp;#10;    case _:&amp;#10;        print(&quot;oops&quot;)&amp;#10;thisExp.addData(&quot;redself&quot;, redself)&amp;#10;thisExp.addData(&quot;bluefriend&quot;, bluefriend)&amp;#10;thisExp.addData(&quot;greenother&quot;, greenother)&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="/* Syntax Error: Fix Python code */" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
